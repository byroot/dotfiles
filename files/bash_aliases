# Force verbose mode
alias ln='ln -v'
alias cp='cp -v'
alias mv='mv -v'
alias rm='rm -v'

# Utils shortcuts
# alias gti='git'
alias my='mysql -u root'
alias ls='ls -G'
alias la='ls -a -G'
alias l='ls -l -G'

# OSX native profiler
alias mprof="xctrace record --template 'Time Profiler' --target-stdout - --launch -- "
